<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.medicinealertapplication">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Medicine Alert"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KnowledgeActivity"></activity>
        <activity android:name=".User.LogoutActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".Alarm.EditTimeActivity" />
        <activity android:name=".Alarm.TimeMedActivity" />
        <activity android:name=".Alarm.SetAlarmActivity" />
        <activity android:name=".YourMedicine.YourMedicineActivity" />
        <activity android:name=".YourMedicine.EditMedicineActivity" />
        <activity android:name=".YourMedicine.AddMedicineActivity" />
        <activity android:name=".AllMedicine.ViewAllMedActivity" />
        <activity android:name=".AllMedicine.AllMedActivity" />
        <activity android:name=".User.SetTimeToEatActivity" />
        <activity android:name=".User.UserActivity" />
        <activity android:name=".User.LoginActivity" />
        <activity android:name=".User.RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Alarm.AlertReceiver" />
    </application>

</manifest>